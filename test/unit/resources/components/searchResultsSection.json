{
    "selector": "div.col-sm-12",
    "children": {
        "Results Number": {
            "selector": "h2"
        },
        "Autosuggestion": {
            "selector": "p.didUmean",
            "children": {
                "Suggestions": {
                    "isCollection": true,
                    "selector": "a.pointer i.ng-binding"
                }
            }
        },
        "Results": {
            "isCollection": true,
            "selector": "div.search-result-item > a",
            "children": {
                "Title": {
                    "selector": "h4.ng-binding"
                },
                "URL": {
                    "selector": "p.search-url"
                },
                "Text": {
                    "selector": "p:nth-of-type(2)"
                }
            }
        },
        "Next Button": {
            "selector": "a.btn[ng-click = 'nextClick()']"
        },
        "Previous Button": {
            "selector": "a.btn[ng-click = 'prevClick()']"
        }
    }
}