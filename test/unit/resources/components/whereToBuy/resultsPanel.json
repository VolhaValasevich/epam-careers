{
    "selector": "div.results",
    "children": {
        "Results Number": {
            "selector": "div.results > h3"
        },
        "Results": {
            "isCollection": true,
            "selector": "div.address",
            "children": {
                "Name": {
                    "selector": "h4[ng-if = '!reseller.Url']"
                },
                "Address": {
                    "selector": "div[ng-if = 'reseller.address'] span"
                },
                "Phone": {
                    "selector": "div[ng-if = 'reseller.phone'] span"
                },
                "Fax": {
                    "selector": "div[ng-if = 'reseller.fax'] span"
                },
                "Email": {
                    "selector": "div[ng-if = 'reseller.email'] a"
                }
            }
        }
    }
}